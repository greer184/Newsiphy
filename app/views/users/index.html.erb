<h3>All users</h3>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
      <% !current_user?(user) %>
        | <%= link_to "delete", user, method: :delete, 
	                              data: { confirm: "Are you sure?" } %>  
      <% end %>
    </li>
  <% end %>
</ul>
